{
  "version": 2,
  "builds": [
    {
      "src": "src/serverless.ts",
      "use": "@vercel/node"
    } 
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "/.vercel/output/functions/src/serverless.js",
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "DELETE"
      ]
    }
  ]
}