{
  "buildCommand": "npm run build",
  "outputDirectory": "/build",
  "framework": "null",
  "builds": [
      {
          "src": "src/api/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)", 
      "destination": "src/api"
    }
  ]
}